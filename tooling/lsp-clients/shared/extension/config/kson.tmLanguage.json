{
  "scopeName": "source.kson",
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "include": "#embed-blocks"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#keywords"
    },
    {
      "include": "#numbers"
    },
    {
      "include": "#operators"
    }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.line.number-sign.kson",
          "match": "#.*$"
        }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.quoted.double.kson",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.kson",
              "match": "\\\\."
            }
          ]
        },
        {
          "name": "string.quoted.single.kson",
          "begin": "'",
          "end": "'",
          "patterns": [
            {
              "name": "constant.character.escape.kson",
              "match": "\\\\."
            }
          ]
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "constant.language.kson",
          "match": "\\b(true|false|null)\\b"
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.kson",
          "match": "-?\\b[0-9]+(\\.[0-9]+)?([eE][+-]?[0-9]+)?\\b"
        }
      ]
    },
    "operators": {
      "patterns": [
        {
          "name": "punctuation.separator.kson",
          "match": "[,:]"
        },
        {
          "name": "punctuation.definition.block.kson",
          "match": "[{}\\[\\]]"
        }
      ]
    },
